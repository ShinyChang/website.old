<?xml version="1.0" encoding="UTF-8"?>
<urlset
  xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9
        http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
<url>
    <loc>http://shinychang.net/about</loc>
    <changefreq>hourly</changefreq>
</url>
<url>
    <loc>http://shinychang.net/article/list</loc>
    <changefreq>hourly</changefreq>
</url>
<url>
    <loc>http://shinychang.net/article/archive</loc>
    <changefreq>hourly</changefreq>
</url>
<% for (var i = 1; i <= page; i++) { %>
<url>
    <loc>http://shinychang.net/article/list?page=<%= i %></loc>
    <changefreq>hourly</changefreq>
</url>
<% } %>
<% for (var o in articles) { %>
    <url>
        <loc>http://shinychang.net/article/<%= articles[o].id %></loc>
        <changefreq>hourly</changefreq>
    </url>
<% } %>
</urlset>